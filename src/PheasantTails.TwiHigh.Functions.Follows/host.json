{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:02",
      "visibilityTimeout": "00:00:05",
      "batchSize": 32,
      "maxDequeueCount": 5,
      "newBatchThreshold": 8
    }
  },
  "logLevel": {
    "FollowFunction": "Information"
  }
}